{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "ignoreExportsUsedInFile": {
    "interface": true,
    "type": true
  },
  "ignoreFiles": ["src/*", "src/demo/**", "src/demo.ts"],
  "ignoreDependencies": ["@tailwindcss/cli", "@vee-validate/zod"],
  "ignoreUnresolved": ["^\\./vitetestPlugins\\.setup\\.ts$", "^\\./\\.storybook/vitest\\.setup\\.ts$"],
  "ignoreIssues": {
    "src/demo/**": ["types", "exports"]
  },
  "tags": [
    "-lintignore"
  ],
  "storybook": {
    "config": [".storybook/main.ts"],
    "entry": [".storybook/preview.ts", "src/**/*.stories.@(js|jsx|ts|tsx)"]
  }
}
