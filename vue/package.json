{
  "name": "@nside/wefa",
  "version": "0.3.0",
  "type": "module",
  "main": "./dist/lib.umd.cjs",
  "module": "./dist/lib.js",
  "private": false,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/n-side-dev/wefa.git"
  },
  "bugs": {
    "url": "https://github.com/n-side-dev/wefa/issues"
  },
  "homepage": "https://github.com/n-side-dev/wefa#readme",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "wefa-install": "./scripts/install.js"
  },
  "files": [
    "dist",
    "src",
    "scripts"
  ],
  "exports": {
    ".": {
      "import": "./dist/lib.js",
      "require": "./dist/lib.cjs"
    },
    "./router": {
      "import": "./dist/router.js",
      "require": "./dist/router.cjs"
    },
    "./containers": {
      "import": "./dist/containers.js",
      "require": "./dist/containers.cjs"
    },
    "./network": {
      "import": "./dist/network.js",
      "require": "./dist/network.cjs"
    },
    "./types": "./dist/lib.d.ts",
    "./style": "./src/assets/main.css"
  },
  "types": "./dist/lib.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:demo": "vite build --config vite.demo.config.ts",
    "preview": "npm run build:demo && vite preview --outDir dist-demo",
    "test:unit": "vitest run --no-file-parallelism",
    "test:unit:watch": "vitest --no-file-parallelism",
    "test:e2e": "npm run build:demo && playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint ./src --fix",
    "lint-check": "eslint ./src",
    "format": "prettier --write src/",
    "format-check": "prettier --check src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "openapi-codegen": "npx @hey-api/openapi-ts --input ./src/demo/openapi.yaml --output ./src/demo/openapi --client @hey-api/client-axios",
    "knip": "knip"
  },
  "dependencies": {
    "@primeuix/themes": "^2.0.3",
    "@primevue/forms": "^4.5.4",
    "@tailwindcss/cli": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/vue-query": "^5.92.9",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^14.2.1",
    "axios": "^1.13.5",
    "luxon": "^3.7.2",
    "marked": "^17.0.1",
    "pinia": "^3.0.4",
    "plotly.js-dist-min": "^3.3.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.5.4",
    "tailwindcss-primeui": "^0.6.1",
    "vue": "^3.5.28",
    "vue-i18n": "^11.2.8",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.1",
    "@hey-api/openapi-ts": "^0.89.2",
    "@playwright/test": "^1.58.2",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-onboarding": "^10.2.8",
    "@storybook/addon-themes": "^10.2.8",
    "@storybook/addon-vitest": "^10.2.8",
    "@storybook/vue3-vite": "^10.2.8",
    "@tsconfig/node22": "^22.0.5",
    "@types/eslint-plugin-security": "^3.0.1",
    "@types/jsdom": "^27.0.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.2.3",
    "@types/plotly.js-dist-min": "^2.3.4",
    "@vitejs/plugin-vue": "^6.0.4",
    "@vitest/browser": "4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-istanbul": "^4.0.18",
    "@vitest/eslint-plugin": "^1.6.7",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.24",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-jsdoc": "^62.3.0",
    "eslint-plugin-playwright": "^2.5.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.6",
    "eslint-plugin-storybook": "^10.2.8",
    "eslint-plugin-vue": "~10.7.0",
    "jiti": "^2.6.1",
    "jsdom": "^28.0.0",
    "knip": "^5.82.1",
    "npm-run-all2": "^8.0.4",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "prettier": "3.8.1",
    "storybook": "^10.2.0",
    "tailwindcss": "^4.1.12",
    "to-valid-identifier": "^1.0.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^8.0.6",
    "vitest": "^4.0.2",
    "vue-tsc": "^3.2.4"
  },
  "bundlesize": [
    {
      "path": "./dist/**/*.js",
      "maxSize": "2 MB"
    }
  ]
}
