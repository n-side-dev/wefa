{
  "name": "@nside/wefa",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/lib.umd.cjs",
  "module": "./dist/lib.js",
  "private": false,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/n-side-dev/wefa.git"
  },
  "bugs": {
    "url": "https://github.com/n-side-dev/wefa/issues"
  },
  "homepage": "https://github.com/n-side-dev/wefa#readme",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "wefa-install": "./scripts/install.js"
  },
  "files": [
    "dist",
    "src",
    "scripts"
  ],
  "exports": {
    ".": {
      "import": "./dist/lib.js",
      "require": "./dist/lib.cjs"
    },
    "./router": {
      "import": "./dist/router.js",
      "require": "./dist/router.cjs"
    },
    "./containers": {
      "import": "./dist/containers.js",
      "require": "./dist/containers.cjs"
    },
    "./network": {
      "import": "./dist/network.js",
      "require": "./dist/network.cjs"
    },
    "./types": "./dist/lib.d.ts",
    "./style": "./src/assets/main.css"
  },
  "types": "./dist/lib.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:demo": "vite build --config vite.demo.config.ts",
    "preview": "npm run build:demo && vite preview --outDir dist-demo",
    "test:unit": "vitest run --no-file-parallelism",
    "test:unit:watch": "vitest --no-file-parallelism",
    "test:e2e": "npm run build:demo && playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint ./src --fix",
    "lint-check": "eslint ./src",
    "format": "prettier --write src/",
    "format-check": "prettier --check src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "openapi-codegen": "npx @hey-api/openapi-ts --input ./src/demo/openapi.yaml --output ./src/demo/openapi --client @hey-api/client-axios"
  },
  "dependencies": {
    "@primeuix/themes": "^1.2.5",
    "@primevue/forms": "^4.4.1",
    "@tailwindcss/vite": "^4.1.16",
    "@tailwindcss/cli": "^4.1.16",
    "@tanstack/vue-query": "^5.90.5",
    "@vee-validate/zod": "^4.15.1",
    "axios": "^1.13.1",
    "markdown-it": "^14.1.0",
    "marked": "^16.4.1",
    "pinia": "^3.0.3",
    "plotly.js-dist-min": "^3.2.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.4.1",
    "tailwindcss-primeui": "^0.6.1",
    "vue": "^3.5.22",
    "vue-i18n": "^11.1.11",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@hey-api/openapi-ts": "^0.86.11",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-a11y": "^10.0.2",
    "@storybook/addon-docs": "^10.0.2",
    "@storybook/addon-onboarding": "^10.0.2",
    "@storybook/addon-themes": "^10.0.2",
    "@storybook/addon-vitest": "^10.0.2",
    "@storybook/vue3-vite": "^10.0.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.9.2",
    "@types/plotly.js-dist-min": "^2.3.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser": "4.0.5",
    "@vitest/browser-playwright": "^4.0.5",
    "@vitest/coverage-istanbul": "^4.0.5",
    "@vitest/eslint-plugin": "^1.4.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "autoprefixer": "^10.4.21",
    "axios-mock-adapter": "^2.1.0",
    "eslint": "^9.38.0",
    "eslint-plugin-jsdoc": "^61.1.11",
    "to-valid-identifier": "^1.0.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-storybook": "^10.0.2",
    "eslint-plugin-vue": "~10.5.1",
    "jiti": "^2.6.1",
    "jsdom": "^27.0.1",
    "npm-run-all2": "^8.0.4",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "storybook": "^10.0.0",
    "tailwindcss": "^4.1.12",
    "typescript": "~5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vitest": "^4.0.2",
    "vue-tsc": "^3.1.2"
  },
  "bundlesize": [
    {
      "path": "./dist/**/*.js",
      "maxSize": "2 MB"
    }
  ]
}
